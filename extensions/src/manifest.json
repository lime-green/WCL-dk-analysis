{
  "manifest_version": 3,
  "name": "WCL DK Analysis",
  "description": "Tool to analyze WCL logs for Classic WoW Frost and Unholy DKs",
  "version": "1.0.4",
  "action": {
    "default_icon": "icon_128.png"
  },
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "content_scripts": [{
    "js": ["content.js"],
    "matches": [
      "https://*.classic.warcraftlogs.com/*",
      "https://classic.warcraftlogs.com/*"
    ]
  }]
}
